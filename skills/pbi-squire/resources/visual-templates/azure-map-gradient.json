{
  "$schema": "https://developer.microsoft.com/json-schemas/fabric/item/report/definition/visualContainer/2.4.0/schema.json",
  "name": "{{VISUAL_NAME}}",
  "position": {
    "x": "{{X}}",
    "y": "{{Y}}",
    "z": "{{Z}}",
    "height": "{{HEIGHT}}",
    "width": "{{WIDTH}}",
    "tabOrder": "{{TAB_ORDER}}"
  },
  "visual": {
    "visualType": "azureMap",
    "query": {
      "queryState": {
        "Category": {
          "projections": [
            {
              "field": {
                "Column": {
                  "Expression": {
                    "SourceRef": {
                      "Entity": "{{LOCATION_TABLE}}"
                    }
                  },
                  "Property": "{{LOCATION_COLUMN}}"
                }
              },
              "queryRef": "{{LOCATION_TABLE}}.{{LOCATION_COLUMN}}",
              "nativeQueryRef": "{{LOCATION_COLUMN}}",
              "active": true
            }
          ]
        },
        "Tooltips": {
          "projections": [
            {
              "field": {
                "Measure": {
                  "Expression": {
                    "SourceRef": {
                      "Entity": "{{MEASURE_TABLE}}"
                    }
                  },
                  "Property": "{{MEASURE_NAME}}"
                }
              },
              "queryRef": "{{MEASURE_TABLE}}.{{MEASURE_NAME}}",
              "nativeQueryRef": "{{MEASURE_NAME}}"
            }
          ]
        }
      }
    },
    "objects": {
      "mapControls": [
        {
          "properties": {
            "defaultStyle": {
              "expr": {
                "Literal": {
                  "Value": "'road'"
                }
              }
            },
            "showStylePicker": {
              "expr": {
                "Literal": {
                  "Value": "false"
                }
              }
            },
            "showNavigationControls": {
              "expr": {
                "Literal": {
                  "Value": "false"
                }
              }
            },
            "showSelectionControl": {
              "expr": {
                "Literal": {
                  "Value": "false"
                }
              }
            }
          }
        }
      ],
      "categoryLabels": [
        {
          "properties": {
            "show": {
              "expr": {
                "Literal": {
                  "Value": "false"
                }
              }
            }
          }
        }
      ],
      "bubbleLayer": [
        {
          "properties": {
            "show": {
              "expr": {
                "Literal": {
                  "Value": "false"
                }
              }
            }
          }
        }
      ],
      "filledMap": [
        {
          "properties": {
            "show": {
              "expr": {
                "Literal": {
                  "Value": "true"
                }
              }
            },
            "mapTransparency": {
              "expr": {
                "Literal": {
                  "Value": "{{MAP_TRANSPARENCY}}L"
                }
              }
            }
          }
        }
      ],
      "dataPoint": [
        {
          "properties": {
            "fill": {
              "solid": {
                "color": {
                  "expr": {
                    "FillRule": {
                      "Input": {
                        "Measure": {
                          "Expression": {
                            "SourceRef": {
                              "Entity": "{{MEASURE_TABLE}}"
                            }
                          },
                          "Property": "{{MEASURE_NAME}}"
                        }
                      },
                      "FillRule": {
                        "linearGradient2": {
                          "min": {
                            "color": {
                              "Literal": {
                                "Value": "'minColor'"
                              }
                            }
                          },
                          "max": {
                            "color": {
                              "Literal": {
                                "Value": "'maxColor'"
                              }
                            }
                          },
                          "nullColoringStrategy": {
                            "strategy": {
                              "Literal": {
                                "Value": "'asZero'"
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "selector": {
            "data": [
              {
                "dataViewWildcard": {
                  "matchingOption": 1
                }
              }
            ]
          }
        }
      ]
    },
    "visualContainerObjects": {
      "title": [
        {
          "properties": {
            "show": {
              "expr": {
                "Literal": {
                  "Value": "true"
                }
              }
            },
            "text": {
              "expr": {
                "Literal": {
                  "Value": "'{{TITLE}}'"
                }
              }
            }
          }
        }
      ]
    }
  },
  "filterConfig": {
    "filters": [
      {
        "name": "{{FILTER_GUID_1}}",
        "field": {
          "Column": {
            "Expression": {
              "SourceRef": {
                "Entity": "{{LOCATION_TABLE}}"
              }
            },
            "Property": "{{LOCATION_COLUMN}}"
          }
        },
        "type": "Categorical"
      },
      {
        "name": "{{FILTER_GUID_2}}",
        "field": {
          "Measure": {
            "Expression": {
              "SourceRef": {
                "Entity": "{{MEASURE_TABLE}}"
              }
            },
            "Property": "{{MEASURE_NAME}}"
          }
        },
        "type": "Advanced"
      }
    ]
  }
}
